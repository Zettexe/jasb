{
  "name": "server",
  "description": "Server.",
  "version": "1.0.0",
  "main": "src/ts/index.ts",
  "engines": {
    "node": ">=15.0.0 <16.0.0"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "prestart": "npm run build",
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "preinspect": "npm run build",
    "inspect": "node --inspect-brk --es-module-specifier-resolution=node dist/index.js",
    "dev": "nodemon --exec \"npm run start\"",
    "debug": "nodemon --exec \"npm run inspect\""
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.9.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "discord-oauth2": "^2.6.0",
    "express": "^5.0.0-alpha.8",
    "express-winston": "^4.0.5",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jose": "^3.6.2",
    "luxon": "^1.26.0",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/luxon": "^1.26.0",
    "@types/source-map-support": "^0.5.3",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.5"
  },
  "nodemonConfig": {
    "ext": "ts,js,json,json5",
    "ignore": [
      "dist"
    ]
  }
}
